{
  "name": "@whl/db",
  "version": "0.1.0",
  "private": true,
  "main": "./index.js",
  "types": "./index.js",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "postinstall": "pnpm with-env prisma generate",
    "typecheck": "tsc --noEmit",
    "generate": "pnpm with-env prisma generate",
    "push": "pnpm with-env prisma db push --skip-generate",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^5.4.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.5.7",
    "@whl/eslint-config": "workspace:*",
    "@whl/prettier-config": "workspace:*",
    "@whl/tsconfig": "workspace:*",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "prisma": "^5.4.1",
    "typescript": "^5.2.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.19.0"
  },
  "prettier": "@whl/prettier-config"
}
