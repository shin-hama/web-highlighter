    "@whl/db": "workspace:*",
    "@whl/ui": "workspace:*",
    "lucide-react": "^0.274.0",
    "plasmo": "0.84.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.243",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@whl/eslint-config": "workspace:*",
    "@whl/prettier-config": "workspace:*",
    "@whl/tsconfig": "workspace:*",
    "autoprefixer": "^10.4.14",
    "postcss": "8.4.28",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
